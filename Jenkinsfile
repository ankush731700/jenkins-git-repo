pipeline {
    agent any
    stages {
        stage("Pull-ankush") { 
            steps {
                echo 'pull a code index.html  github'
            }
        }    
        stage("build-ankush") {
            steps {
                echo 'build a code index.html'
            }
        }
        
        stage("Test-ankush") { 
            steps {
                echo 'Test a code index.html'
            }
        }    
       
        stage("Deploy-ankush") {    
            steps {
                echo 'Deploy a server'
            }
        }
    }
}
